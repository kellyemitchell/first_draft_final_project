<form action="/create_photo_from_venue" method="post">

  <div class="form-group">
    <label for="user_id">
      User
    </label>

    <%= select_tag(:user_id, options_from_collection_for_select(User.all, :id, :email, @photo.user_id), :class => "form-control") %>
  </div>

  <div class="form-group">
    <label for="date">
      Date
    </label>

    <input type="text" id="date" name="date" class="form-control" value="<%= @photo.date %>">
  </div>

  <div class="form-group">
    <label for="caption">
      Caption
    </label>

    <input type="text" id="caption" name="caption" class="form-control" value="<%= @photo.caption %>">
  </div>

  <input type="hidden" name="venue_id" value="<%= @venue.id %>">

  <div class="form-group">
    <label for="photo_location">
      Photo location
    </label>

    <input type="text" id="photo_location" name="photo_location" class="form-control" value="<%= @photo.photo_location %>">
  </div>

  <button class="btn btn-block btn-outline-secondary">
    Create photo
  </button>
</form>
